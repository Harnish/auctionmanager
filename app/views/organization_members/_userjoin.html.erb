<%= form_for(@organization_members) do |f| %>
  <div class="field">
    <%= f.label :organization_id %><br />
    <%= f.text_field :organization_id %>
  </div>
  <div class="field">
    <%= f.collection_select( :user_id, Users.all, :id, :email)  %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>

<% end %>
